#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/hz-ui/packages/components/bin/node_modules:/mnt/d/hz-ui/packages/components/node_modules:/mnt/d/hz-ui/packages/node_modules:/mnt/d/hz-ui/node_modules:/mnt/d/node_modules:/mnt/d/hz-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/hz-ui/packages/components/bin/node_modules:/mnt/d/hz-ui/packages/components/node_modules:/mnt/d/hz-ui/packages/node_modules:/mnt/d/hz-ui/node_modules:/mnt/d/node_modules:/mnt/d/hz-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../packages/components/bin/vue-fix.js" "$@"
else
  exec node  "$basedir/../../../packages/components/bin/vue-fix.js" "$@"
fi
